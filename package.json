{
  "name": "autoeq-ui",
  "private": true,
  "version": "0.2.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:audio-player": "node src/modules/audio/build-audio-player.js",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest --run",
    "test:unit": "vitest --run --exclude '**/e2e/**'",
    "test:e2e": "vitest --run src/tests/e2e",
    "test:e2e:force": "FORCE_E2E=true vitest --run src/tests/e2e",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "fmt": "npx prettier -w src-ui",
    "upgrade": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/plotly.js": "^3.0.6",
    "chrome-devtools-mcp": "^0.6.0",
    "plotly.js-dist-min": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tauri-apps/cli": "^2.8.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "markdownlint-cli2": "^0.18.1",
    "npm-check-updates": "^19.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4"
  }
}
